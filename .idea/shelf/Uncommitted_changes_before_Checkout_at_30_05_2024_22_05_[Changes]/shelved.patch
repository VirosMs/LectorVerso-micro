+package org.virosms.lectorversomicro.service;
+
+import static org.junit.jupiter.api.Assertions.*;
+import static org.mockito.Mockito.*;
+
+import org.junit.jupiter.api.BeforeEach;
+import org.junit.jupiter.api.Test;
+import org.junit.jupiter.api.extension.ExtendWith;
+import org.mockito.InjectMocks;
+import org.mockito.Mock;
+import org.mockito.junit.jupiter.MockitoExtension;
+import org.springframework.http.HttpStatus;
+import org.springframework.http.ResponseEntity;
+import org.virosms.lectorversomicro.entity.User;
+import org.virosms.lectorversomicro.errors.LVException;
+import org.virosms.lectorversomicro.errors.LectorVersoExceptions;
+import org.virosms.lectorversomicro.errors.LeverErrorType;
+import org.virosms.lectorversomicro.repository.UserRepository;
+
+@ExtendWith(MockitoExtension.class)
+public class UserServiceTest {
+
+    @Mock
+    private UserRepository userRepository;
+
+    @InjectMocks
+    private UserService userService;
+
+    private User validUser;
+
+    @BeforeEach
+    void setUp() {
+        validUser = new User();
+        validUser.setId(1L);
+        validUser.setName("John Doe");
+        validUser.setEmail("john.doe@example.com");
+        validUser.setPassword("password123");
+    }
+
+    @Test
+    void testCreateUserSuccess() throws LVException {
+        when(userRepository.existsByName(validUser.getName())).thenReturn(false);
+        when(userRepository.existsByEmail(validUser.getEmail())).thenReturn(false);
+
+        ResponseEntity<?> response = userService.createUser(validUser);
+
+        assertEquals(HttpStatus.CREATED, response.getStatusCode());
+        verify(userRepository).save(validUser);
+    }
+
+    @Test
+    void testCreateUserThrowsParameterRequiredException() {
+        User invalidUser = new User();
+
+        LVException exception = assertThrows(LVException.class, () -> userService.createUser(invalidUser));
+
+        LeverErrorType expectedErrorType = LectorVersoExceptions.PARAMETER_REQUIRED;
+
+        assertEquals(expectedErrorType.getDetail(), exception.getDetail());
+        assertEquals(expectedErrorType.getStatus(), exception.getStatus());
+        assertEquals(expectedErrorType.getType(), exception.getType());
+    }
+
+    @Test
+    void testCreateUserThrowsUserAlreadyExistsException() {
+        when(userRepository.existsByName(validUser.getName())).thenReturn(true);
+
+        LVException exception = assertThrows(LVException.class, () -> userService.createUser(validUser));
+
+        LeverErrorType expectedErrorType = LectorVersoExceptions.USER_ALREADY_EXISTS;
+
+        assertEquals(expectedErrorType.getDetail(), exception.getDetail());
+        assertEquals(expectedErrorType.getStatus(), exception.getStatus());
+        assertEquals(expectedErrorType.getType(), exception.getType());
+    }
+
+    @Test
+    void testCreateUserThrowsExceptionWhenSaveFails() {
+        when(userRepository.existsByName(validUser.getName())).thenReturn(false);
+        when(userRepository.existsByEmail(validUser.getEmail())).thenReturn(false);
+        doThrow(new RuntimeException("Save failed")).when(userRepository).save(validUser);
+
+        LVException exception = assertThrows(LVException.class, () -> userService.createUser(validUser));
+
+        LeverErrorType expectedErrorType = LectorVersoExceptions.USER_NOT_CREATED;
+
+        assertEquals(expectedErrorType.getDetail(), exception.getDetail());
+        assertEquals(expectedErrorType.getStatus(), exception.getStatus());
+        assertEquals(expectedErrorType.getType(), exception.getType());
+    }
+}
